{
    "name": "SourceCollectionMap",
    "value": [
        {
            "coll": "District",
            "filter": {
                "SAV_KODAS": {
                    "$ne": null
                }
            },
            "unset": [
                "SAV_KODAS"
            ]
        },
        {
            "coll": "Region",
            "rels": [
                {
                    "field": "districtId",
                    "from": "District"
                },
                {
                    "field": "localityId",
                    "from": "Locality",
                    "optional": true
                }
            ],
            "unset": [
                "SAV_KODAS",
                "SEN_KODAS"
            ]
        },
        {
            "coll": "Locality",
            "filter": {
                "GYV_KODAS": {
                    "$ne": null
                }
            },
            "rels": [
                {
                    "field": "districtId",
                    "from": "District"
                },
                {
                    "field": "regionId",
                    "from": "Region",
                    "optional": true
                }
            ],
            "unset": [
                "GYV_KODAS",
                "district_id",
                "streetCount"
            ]
        },
        {
            "coll": "Street",
            "filter": {
                "GAT_KODAS": {
                    "$ne": null
                }
            },
            "rels": [
                {
                    "field": "localityId",
                    "from": "Locality"
                },
                {
                    "field": "regionId",
                    "from": "Region",
                    "optional": true
                }
            ],
            "unset": [
                "GAT_KODAS",
                "locality_id"
            ]
        },
        {
            "coll": "Person",
            "filter": {},
            "pre": [
                { 
                    "$addFields": { 
                        "type": "person",
                        "name": { 
                            "$concat": [
                                "$firstName",
                                " ",
                                "$lastName"
                            ]
                        }
                    } 
                },
                {   
                    "$unionWith": {
                        "coll": "LegalEntity",
                        "pipeline": [
                            { "$addFields": { "type": "entity" } }
                        ]
                    }
                }
            ],
            "rename": "Customer",
            "unset": [
                "notificationAgreement",
                "servingMasterId",
                "locationId"
            ]
        },
        {
            "coll": "ServicePoint",
            "filter": {
                "siteId": "2b1f36e3-8506-451f-9cfa-d62bf8e0aa49"
            },
            "pre": [
                {
                    "$lookup": {
                        "as": "items",
                        "foreignField": "servicePointId",
                        "from": "ServiceItem",
                        "localField": "id"
                    }
                },
                {
                    "$project": {
                        "items": "$items"
                    }
                },
                {
                    "$unwind": "$items"
                },
                {
                    "$match": {
                        "items.servingMasterId": {
                            "$ne": null
                        }
                    }
                },
                {
                    "$group": {
                        "_id": "$_id",
                        "masterIds": {
                            "$addToSet": "$items.servingMasterId"
                        }
                    }
                },
                {
                    "$lookup": {
                        "as": "point",
                        "foreignField": "_id",
                        "from": "ServicePoint",
                        "localField": "_id"
                    }
                },
                {
                    "$unwind": "$point"
                },
                {
                    "$set": {
                        "point.masterIds": "$masterIds"
                    }
                },
                {
                    "$replaceRoot": {
                        "newRoot": "$point"
                    }
                }
            ],
            "rels": [
                {
                    "field": "localityId",
                    "from": "Locality",
                    "optional": true
                },
                {
                    "field": "streetId",
                    "from": "Street",
                    "optional": true
                }
            ],
            "unset": [
                "geo",
                "servingMasterId",
                "currentServiceContractId",
                "locationId",
                "isPaused",
                "isReady",
                "phone",
                "type",
                "gifts",
                "siteId"
            ]
        },
        {
            "coll": "ServiceItem",
            "filter": {
                "sp.siteId": "2b1f36e3-8506-451f-9cfa-d62bf8e0aa49"
            },
            "pre": [
                {
                    "$lookup": {
                        "as": "sp",
                        "foreignField": "id",
                        "from": "ServicePoint",
                        "localField": "servicePointId"
                    }
                },
                {
                    "$unwind": "$sp"
                }
            ],
            "rels": [
                {
                    "field": "servicePointId",
                    "from": "ServicePoint"
                },
                {
                    "field": "servingMasterId",
                    "from": "Employee",
                    "optional": true
                },
                {
                    "field": "installingMasterId",
                    "from": "Employee",
                    "optional": true
                }
            ],
            "unset": [
                "nextServiceDate",
                "plannedServiceDate",
                "plannedServiceType",
                "lastServiceType",
                "lastServiceDate",
                "pausedFrom",
                "currentServiceContractId",
                "site",
                "sp"
            ]
        },
        {
            "coll": "Employee",
            "filter": {
                "siteId": "2b1f36e3-8506-451f-9cfa-d62bf8e0aa49"
            },
            "pre": [
                {
                    "$lookup": {
                        "as": "person",
                        "foreignField": "id",
                        "from": "Person",
                        "localField": "personId"
                    }
                },
                {
                    "$unwind": "$person"
                },
                {
                    "$set": {
                        "name": {
                            "$concat": [
                                "$person.firstName",
                                " ",
                                "$person.lastName"
                            ]
                        }
                    }
                }
            ],
            "rename": "Master",
            "unset": [
                "personId",
                "siteId",
                "accountId",
                "person",
                "info"
            ]
        }
    ],
    "from_secret": false
}
