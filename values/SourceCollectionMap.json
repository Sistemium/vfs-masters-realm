{
    "name": "SourceCollectionMap",
    "value": [
        {
            "coll": "FilterSystemType"
        },
        {
            "coll": "District",
            "filter": {
                "SAV_KODAS": {
                    "$ne": null
                }
            },
            "unset": [
                "SAV_KODAS"
            ]
        },
        {
            "coll": "Region",
            "rels": [
                {
                    "field": "districtId",
                    "from": "District"
                },
                {
                    "field": "localityId",
                    "from": "Locality",
                    "optional": true
                }
            ],
            "unset": [
                "SAV_KODAS",
                "SEN_KODAS"
            ]
        },
        {
            "coll": "Locality",
            "filter": {
                "GYV_KODAS": {
                    "$ne": null
                }
            },
            "rels": [
                {
                    "field": "districtId",
                    "from": "District"
                },
                {
                    "field": "regionId",
                    "from": "Region",
                    "optional": true
                }
            ],
            "unset": [
                "GYV_KODAS",
                "district_id",
                "streetCount"
            ]
        },
        {
            "coll": "Street",
            "filter": {
                "GAT_KODAS": {
                    "$ne": null
                }
            },
            "rels": [
                {
                    "field": "localityId",
                    "from": "Locality"
                },
                {
                    "field": "regionId",
                    "from": "Region",
                    "optional": true
                }
            ],
            "unset": [
                "GAT_KODAS",
                "locality_id"
            ]
        },
        {
            "coll": "Person",
            "filter": {},
            "pre": [
                {
                    "$addFields": {
                        "name": {
                            "$concat": [
                                "$firstName",
                                " ",
                                "$lastName"
                            ]
                        },
                        "type": "person"
                    }
                },
                {
                    "$lookup": {
                        "as": "contracts",
                        "foreignField": "customerPersonId",
                        "from": "ServiceContract",
                        "localField": "id"
                    }
                },
                {
                    "$unionWith": {
                        "coll": "LegalEntity",
                        "pipeline": [
                            {
                                "$addFields": {
                                    "type": "entity"
                                }
                            },
                            {
                                "$lookup": {
                                    "as": "contracts",
                                    "foreignField": "customerLegalEntityId",
                                    "from": "ServiceContract",
                                    "localField": "id"
                                }
                            }
                        ]
                    }
                },
                {
                    "$addFields": {
                        "paymentMethod": {
                            "$arrayElemAt": [
                                "$contracts.paymentMethod",
                                {
                                    "$numberInt": "0"
                                }
                            ]
                        }
                    }
                }
            ],
            "rels": [
                {
                    "field": "localityId",
                    "from": "Locality",
                    "optional": true
                },
                {
                    "field": "streetId",
                    "from": "Street",
                    "optional": true
                }
            ],
            "rename": "Customer",
            "unset": [
                "notificationAgreement",
                "servingMasterId",
                "locationId",
                "contracts",
                "phone",
                "email",
                "avatarPictureId"
            ]
        },
        {
            "coll": "Site",
            "filter": {
                "id": {
                    "$in": [
                        "2b1f36e3-8506-451f-9cfa-d62bf8e0aa49",
                        "805b2850-ba6f-4231-b531-48773134a089",
                        "549984fd-afb9-4386-9dc0-9d8664f134d5"
                    ]
                }
            }
        },
        {
            "coll": "ServicePoint",
            "filter": {
                "siteId": {
                    "$in": [
                        "2b1f36e3-8506-451f-9cfa-d62bf8e0aa49",
                        "805b2850-ba6f-4231-b531-48773134a089",
                        "549984fd-afb9-4386-9dc0-9d8664f134d5"
                    ]
                }
            },
            "pre": [
                {
                    "$lookup": {
                        "as": "items",
                        "foreignField": "servicePointId",
                        "from": "ServiceItem",
                        "localField": "id"
                    }
                },
                {
                    "$project": {
                        "items": "$items"
                    }
                },
                {
                    "$unwind": "$items"
                },
                {
                    "$match": {
                        "items.servingMasterId": {
                            "$ne": null
                        }
                    }
                },
                {
                    "$lookup": {
                        "as": "emp",
                        "foreignField": "id",
                        "from": "Employee",
                        "localField": "items.servingMasterId"
                    }
                },
                {
                    "$unwind": "$emp"
                },
                {
                    "$group": {
                        "_id": "$_id",
                        "masterIds": {
                            "$addToSet": "$emp._id"
                        }
                    }
                },
                {
                    "$lookup": {
                        "as": "point",
                        "foreignField": "_id",
                        "from": "ServicePoint",
                        "localField": "_id"
                    }
                },
                {
                    "$unwind": "$point"
                },
                {
                    "$set": {
                        "point.masterIds": "$masterIds"
                    }
                },
                {
                    "$replaceRoot": {
                        "newRoot": "$point"
                    }
                },
                {
                    "$lookup": {
                        "as": "contract",
                        "foreignField": "id",
                        "from": "ServiceContract",
                        "localField": "currentServiceContractId"
                    }
                },
                {
                    "$unwind": "$contract"
                },
                {
                    "$lookup": {
                        "as": "person",
                        "foreignField": "id",
                        "from": "Person",
                        "localField": "contract.customerPersonId"
                    }
                },
                {
                    "$lookup": {
                        "as": "entity",
                        "foreignField": "id",
                        "from": "LegalEntity",
                        "localField": "contract.customerLegalEntityId"
                    }
                },
                {
                    "$unwind": {
                        "path": "$entity",
                        "preserveNullAndEmptyArrays": true
                    }
                },
                {
                    "$unwind": {
                        "path": "$person",
                        "preserveNullAndEmptyArrays": true
                    }
                },
                {
                    "$set": {
                        "customer": {
                            "$ifNull": [
                                "$person._id",
                                "$entity._id"
                            ]
                        }
                    }
                },
                {
                    "$unset": [
                        "contract",
                        "person",
                        "entity"
                    ]
                }
            ],
            "rels": [
                {
                    "field": "localityId",
                    "from": "Locality",
                    "optional": true
                },
                {
                    "field": "streetId",
                    "from": "Street",
                    "optional": true
                },
                {
                    "field": "siteId",
                    "from": "Site"
                }
            ],
            "unset": [
                "geo",
                "servingMasterId",
                "currentServiceContractId",
                "locationId",
                "isPaused",
                "isReady",
                "phone",
                "type",
                "gifts"
            ]
        },
        {
            "coll": "ServiceItem",
            "filter": {
                "sp.siteId": {
                    "$in": [
                        "2b1f36e3-8506-451f-9cfa-d62bf8e0aa49",
                        "805b2850-ba6f-4231-b531-48773134a089",
                        "549984fd-afb9-4386-9dc0-9d8664f134d5"
                    ]
                }
            },
            "pre": [
                {
                    "$lookup": {
                        "as": "sp",
                        "foreignField": "id",
                        "from": "ServicePoint",
                        "localField": "servicePointId"
                    }
                },
                {
                    "$unwind": "$sp"
                },
                {
                    "$lookup": {
                        "as": "fs",
                        "foreignField": "id",
                        "from": "FilterSystem",
                        "localField": "filterSystemId"
                    }
                },
                {
                    "$unwind": "$fs"
                },
                {
                    "$set": {
                        "filterSystemType": "$fs._id",
                        "guaranteePeriod": {
                            "$ifNull": [
                                "$guaranteePeriod",
                                "$fs.guaranteePeriod"
                            ]
                        },
                        "name": "$fs.name",
                        "serviceFrequency": {
                            "$ifNull": [
                                "$serviceFrequency",
                                "$fs.serviceFrequency"
                            ]
                        },
                        "servicePrice": {
                            "$ifNull": [
                                "$servicePrice",
                                "$fs.servicePrice"
                            ]
                        },
                        "smallServicePrice": {
                            "$ifNull": [
                                "$smallServicePrice",
                                "$fs.smallServicePrice"
                            ]
                        }
                    }
                }
            ],
            "rels": [
                {
                    "field": "servicePointId",
                    "from": "ServicePoint"
                },
                {
                    "field": "servingMasterId",
                    "from": "Employee",
                    "optional": true
                },
                {
                    "field": "installingMasterId",
                    "from": "Employee",
                    "optional": true
                }
            ],
            "unset": [
                "nextServiceDate",
                "plannedServiceDate",
                "plannedServiceType",
                "lastServiceType",
                "lastServiceDate",
                "currentServiceContractId",
                "site",
                "sp",
                "fs",
                "filterSystemId"
            ]
        },
        {
            "coll": "Employee",
            "pre": [
                {
                    "$lookup": {
                        "as": "person",
                        "foreignField": "id",
                        "from": "Person",
                        "localField": "personId"
                    }
                },
                {
                    "$unwind": "$person"
                },
                {
                    "$set": {
                        "name": {
                            "$concat": [
                                "$person.firstName",
                                " ",
                                "$person.lastName"
                            ]
                        }
                    }
                }
            ],
            "rels": [
                {
                    "field": "siteId",
                    "from": "Site"
                }
            ],
            "rename": "Master",
            "unset": [
                "personId",
                "accountId",
                "person",
                "info"
            ]
        },
        {
            "coll": "ServicePointContact",
            "pre": [
                {
                    "$lookup": {
                        "as": "person",
                        "foreignField": "id",
                        "from": "Person",
                        "localField": "personId"
                    }
                },
                {
                    "$unwind": "$person"
                },
                {
                    "$set": {
                        "customer": "$person._id"
                    }
                }
            ],
            "rels": [
                {
                    "field": "servicePointId",
                    "from": "ServicePoint"
                },
                {
                    "$unset": [
                        "person"
                    ]
                }
            ]
        },
        {
            "coll": "ContactMethod"
        },
        {
            "coll": "Contact",
            "pre": [
                {
                    "$lookup": {
                        "as": "person",
                        "foreignField": "id",
                        "from": "Person",
                        "localField": "ownerXid"
                    }
                },
                {
                    "$lookup": {
                        "as": "entity",
                        "foreignField": "id",
                        "from": "LegalEntity",
                        "localField": "ownerXid"
                    }
                },
                {
                    "$set": {
                        "customer": {
                            "$ifNull": [
                                {
                                    "$arrayElemAt": [
                                        "$person",
                                        {
                                            "$numberInt": "0"
                                        }
                                    ]
                                },
                                {
                                    "$arrayElemAt": [
                                        "$entity",
                                        {
                                            "$numberInt": "0"
                                        }
                                    ]
                                }
                            ]
                        }
                    }
                },
                {
                    "$set": {
                        "customer": "$customer._id"
                    }
                },
                {
                    "$match": {
                        "customer": {
                            "$ne": null
                        }
                    }
                },
                {
                    "$unset": [
                        "person",
                        "entity",
                        "ownerXid"
                    ]
                }
            ],
            "rels": [
                {
                    "field": "contactMethodId",
                    "from": "ContactMethod"
                }
            ],
            "unset": [
                "source"
            ]
        },
        {
            "coll": "ServiceItemService",
            "filter": {
                "sp.siteId": {
                    "$in": [
                        "2b1f36e3-8506-451f-9cfa-d62bf8e0aa49",
                        "805b2850-ba6f-4231-b531-48773134a089",
                        "549984fd-afb9-4386-9dc0-9d8664f134d5"
                    ]
                }
            },
            "pre": [
                {
                    "$lookup": {
                        "as": "si",
                        "foreignField": "id",
                        "from": "ServiceItem",
                        "localField": "serviceItemId"
                    }
                },
                {
                    "$unwind": "$si"
                },
                {
                    "$lookup": {
                        "as": "sp",
                        "foreignField": "id",
                        "from": "ServicePoint",
                        "localField": "si.servicePointId"
                    }
                },
                {
                    "$unwind": "$sp"
                },
                {
                    "$unset": [
                        "si"
                    ]
                }
            ],
            "rels": [
                {
                    "field": "serviceItemId",
                    "from": "ServiceItem"
                },
                {
                    "field": "servingMasterId",
                    "from": "Employee",
                    "optional": true
                }
            ],
            "unset": [
                "sp"
            ]
        },
        {
            "coll": "StockWithdrawing",
            "pre": [
                {
                    "$match": {
                        "counterpartyType": "Storage"
                    }
                },
                {
                    "$lookup": {
                        "as": "items",
                        "foreignField": "stockWithdrawingId",
                        "from": "StockWithdrawingItem",
                        "localField": "id"
                    }
                },
                {
                    "$unwind": "$items"
                },
                {
                    "$group": {
                        "_id": "$counterpartyId",
                        "articles": {
                            "$addToSet": "$items.articleId"
                        },
                        "cnt": {
                            "$sum": {
                                "$numberInt": "1"
                            }
                        },
                        "md": {
                            "$max": "$date"
                        }
                    }
                },
                {
                    "$lookup": {
                        "as": "s",
                        "foreignField": "id",
                        "from": "Storage",
                        "localField": "_id"
                    }
                },
                {
                    "$unwind": "$s"
                },
                {
                    "$match": {
                        "s.type": "personal"
                    }
                },
                {
                    "$set": {
                        "storage": "$s.id"
                    }
                },
                {
                    "$unwind": "$articles"
                },
                {
                    "$group": {
                        "_id": "$articles",
                        "storageIds": {
                            "$addToSet": "$storage"
                        }
                    }
                },
                {
                    "$lookup": {
                        "as": "a",
                        "foreignField": "id",
                        "from": "Article",
                        "localField": "_id"
                    }
                },
                {
                    "$unwind": "$a"
                },
                {
                    "$set": {
                        "a.storageIds": "$storageIds"
                    }
                },
                {
                    "$replaceRoot": {
                        "newRoot": "$a"
                    }
                },
                {
                    "$match": {
                        "materials": null
                    }
                }
            ],
            "rels": [
                {
                    "field": "avatarPictureId",
                    "from": "Picture"
                }
            ],
            "rename": "Article",
            "unset": [
                "barcodes",
                "boxes",
                "isCustomName",
                "materials",
                "props",
                "isSKU"
            ]
        },
        {
            "coll": "Picture",
            "filter": {
                "target": "Article"
            },
            "pre": [
                {
                    "$lookup": {
                        "as": "a",
                        "foreignField": "id",
                        "from": "Article",
                        "localField": "ownerXid"
                    }
                },
                {
                    "$unwind": "$a"
                },
                {
                    "$set": {
                        "owner": "$a._id"
                    }
                }
            ],
            "unset": [
                "picturesInfo",
                "ownerXid",
                "name",
                "a"
            ]
        }
    ],
    "from_secret": false
}
